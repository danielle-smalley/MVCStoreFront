@model MVCStoreFront.UI.Models.ContactViewModel

@{
    ViewBag.Title = "Confirmation";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="container col-md-12 text-center">
    <h2>@ViewBag.Title</h2>


    <h4>We received the following message: </h4>
    <p>@Model.Message</p>
    <h4>We will respond as soon as we can to @Model.Email  . Thank you for contacting MeowMart!</h4>
    <img src="~/Content/img/registercat.jpg" height="600" width="500" />
</div>

